package euler;

import java.util.ArrayList;
import java.util.Collections;
import java.util.Scanner;

/*
 * all distinct terms in sequence generated by a^b for 2 <= a <= 100 && 2 <= b <= 100
 * https://projecteuler.net/problem=29
 */

public class Problem29_distinctPowers {
	static ArrayList<Integer> combinations = new ArrayList<Integer>();
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		integerCombinations(in.nextInt());
		in.close();
		Collections.sort(combinations);
		System.out.println("Arraylist contains: " + combinations.toString());  
	}
	
	static void integerCombinations(int range) {
		for(int i = 2; i <= range; i++) {
			for(int j = 2; j <= range; j++) {
				combinations.add((int)Math.pow(i, j));
			}
		}
	}
}
