package euler;

import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

/*
 * all distinct terms in sequence generated by a^b for 2 <= a <= 100 && 2 <= b <= 100
 * https://projecteuler.net/problem=29
 */

public class Problem29_distinctPowers {
	static Set<Double> combinations = new HashSet<Double>();
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		integerCombinations(in.nextDouble());
		in.close();
		System.out.println(combinations.size()); 
	}
	
	static void integerCombinations(int range) {
		for(int i = 2; i <= range; i++) {
			for(int j = 2; j <= range; j++) {
				combinations.add(Math.pow(i, j));
			}
		}
	}
}
